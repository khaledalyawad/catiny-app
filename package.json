{
  "name": "catiny-app",
  "version": "0.0.1",
  "scripts": {
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx",
    "test": "jest",
    "publish": "expo publish",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "build:web": "expo build:web",
    "precommit": "lint-staged",
    "start:e2e": "expo start --no-dev --minify",
    "test:e2e": "./e2e/scripts/setup.sh && detox test --configuration ios.sim.release --retries 3",
    "postinstall": "patch-package",
    "prettier": "prettier --write \"{,.,**/,.jhipster/**/}*.{md,json,yml,js,ts,tsx}\"",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "native:android": "npm run gradle:clean && react-native run-android",
    "native:ios": "react-native run-ios",
    "gradle:clean": "cd android && ./gradlew clean && cd ..",
    "native:start:ubuntu": "gnome-terminal -- sh -c \"react-native start\"",
    "linux:android": "chmod +x ./android/gradlew && npm run native:start:ubuntu && npm run native:android"
  },
  "lint-staged": {
    "{,app/**/,test/**/}*.{js,json,yml,md,ts,tsx}": [
      "prettier --write",
      "eslint --fix --ext .js"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "app/shared/services/api.js",
      "app/shared/themes/",
      "/storybook/",
      "/*\\.story\\.js"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "preset": "jest-expo",
    "setupFiles": [
      "./test/setup"
    ],
    "testMatch": [
      "**/test/**/*.js",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e/",
      "test/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native-community|react-native|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|@storybook))"
    ]
  },
